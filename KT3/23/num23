public class Main {
    public static void main(String[] args) {
        int n = 10;
        int a = 2;
        int b = 3;

        int sum = 0;
        for(int i = 1; i <= n; i++) {
            if (i % a == 0 || i % b == 0) {
                sum += i;
            }
        }

        System.out.println(sum);
    }
}
